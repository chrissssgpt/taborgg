// manifest.json
{
    "manifest_version": 2, 
    "name": "Tab Organizer", 
    "description": "Organize your tabs into groups and folders", 
    "permissions": ["tabs", "storage"], 
    "background": {
      "scripts": ["background.js"] 
    }, 
    "options_ui": {
      "page": "options.html" 
    },
    "icons": { 
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png" 
    } 
  }
  